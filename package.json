{
  "name": "callemall-coding-challenge",
  "version": "0.0.1",
  "description": "An application built for the Call-Em-All Coding Challenge",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "main": "server.js",
  "scripts": {
    "build": "next build",
    "dev": "node -r dotenv/config ./server",
    "heroku-postbuild": "npm run build",
    "start": "node ./server",
    "test:dev": "jest --watchAll --runInBand --coverage",
    "test:integrationWithWatch": "jest -c __test__/integration/jest.config.js --watch",
    "test": "jest  --coverage"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smcguinness/callemall-coding-challenge.git"
  },
  "author": "Seth McGuinness <seth.mcguinness@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smcguinness/callemall-coding-challenge/issues"
  },
  "homepage": "https://github.com/smcguinness/callemall-coding-challenge#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "3.0.2",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "express": "4.16.4",
    "helmet": "3.15.0",
    "js-cookie": "2.2.0",
    "jss": "9.8.7",
    "moment": "2.24.0",
    "next": "7.0.2",
    "next-cookies": "1.0.4",
    "prop-types": "15.6.2",
    "query-string": "^5.1.1",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-jss": "8.6.1",
    "styled-jsx": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "dotenv": "6.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "faker": "4.1.0",
    "jest": "24.0.0",
    "jest-puppeteer": "3.9.0",
    "nock": "^10.0.6",
    "puppeteer": "1.12.2",
    "react-addons-test-utils": "15.6.2",
    "sinon": "7.2.3"
  }
